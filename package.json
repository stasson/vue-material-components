{
  "name": "vue-mdc-adapter",
  "version": "0.0.41",
  "description": "Vue wrapper arround Material Components for the Web",
  "keywords": [
    "vue",
    "vuejs",
    "plugin",
    "material",
    "components",
    "adapter",
    "mdc",
    "design"
  ],
  "author": "Sebastien Tasson <sebastien.tasson@gmail.com>",
  "license": "MIT",
  "main": "dist/vue-mdc-adapter.js",
  "module": "components/index.js",
  "unpkg": "dist/vue-mdc-adapter.js",
  "files": [
    "dist",
    "static",
    "components",
    "LICENSE",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/stasson/vue-mdc-adapter.git"
  },
  "bugs": {
    "url": "https://github.com/stasson/vue-mdc-adapter/issues"
  },
  "scripts": {
    "dev": "node build/dev-server.js",
    "start": "node build/dev-server.js",
    "changelog": "standard-changelog -i CHANGELOG.md -k package.json -w",
    "commitmsg": "validate-commit-msg",
    "build": "npm run rollup && npm run build-docs",
    "build-dist": "node build/build-dist.js",
    "build-docs": "node build/build-docs.js",
    "rollup": "npm run rollup-dev && npm run rollup-prod",
    "rollup-dev": "cross-env NODE_ENV=development rollup -c",
    "rollup-prod": "cross-env NODE_ENV=production rollup -c",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "test": "npm run unit",
    "lint": "npm run lint:es && npm run lint:style",
    "lint:es": "eslint --ext .js,.vue components",
    "lint:style": "stylelint \"components/**/*{.scss,.vue}\" "
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "peerDependencies": {
    "vue": "^2.4.4"
  },
  "dependencies": {
    "material-components-web": "0.21.1"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.3",
    "babel-core": "^6.22.1",
    "babel-eslint": "^8.0.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-env": "^1.3.2",
    "babel-register": "^6.22.0",
    "chai": "^4.1.2",
    "chalk": "^2.1.0",
    "connect-history-api-fallback": "^1.3.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.0",
    "cz-conventional-changelog": "^2.0.0",
    "eslint": "^4.7.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-html": "^3.2.2",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.14.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.4",
    "friendly-errors-webpack-plugin": "^1.1.3",
    "html-webpack-plugin": "^2.28.0",
    "http-proxy-middleware": "^0.17.3",
    "husky": "^0.14.3",
    "inject-loader": "^3.0.0",
    "karma": "^1.4.1",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-phantomjs-shim": "^1.4.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.31",
    "karma-webpack": "^2.0.2",
    "mocha": "^3.2.0",
    "node-sass": "^4.5.3",
    "opn": "^5.0.0",
    "optimize-css-assets-webpack-plugin": "^3.1.1",
    "ora": "^1.2.0",
    "phantomjs-prebuilt": "^2.1.14",
    "postcss-csso": "^2.0.0",
    "rimraf": "^2.6.0",
    "rollup": "^0.50.0",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.0",
    "rollup-plugin-filesize": "^1.4.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-sass": "^0.5.3",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-plugin-vue": "^2.5.1",
    "rollup-plugin-vue2": "^0.8.0",
    "sass-loader": "^6.0.5",
    "semver": "^5.3.0",
    "shelljs": "^0.7.6",
    "standard-changelog": "^1.0.3",
    "stylelint": "^8.1.1",
    "stylelint-config-recommended": "^1.0.0",
    "stylelint-processor-html": "^1.0.0",
    "uglify-es": "^3.1.1",
    "url-loader": "^0.5.8",
    "validate-commit-msg": "^2.14.0",
    "vue": "^2.4.2",
    "vue-hot-reload-api": "^2.1.0",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^13.0.5",
    "vue-router": "^2.5.3",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.2.6",
    "webpack": "^3.6.0",
    "webpack-bundle-analyzer": "^2.2.1",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-merge": "^4.1.0"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
