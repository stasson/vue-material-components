{
  "name": "vue-mdc-adapter",
  "version": "0.0.44",
  "description": "Vue wrapper arround Material Components for the Web",
  "keywords": [
    "vue",
    "vuejs",
    "plugin",
    "material",
    "components",
    "adapter",
    "mdc",
    "design"
  ],
  "author": "Sebastien Tasson <sebastien.tasson@gmail.com>",
  "license": "MIT",
  "main": "dist/vue-mdc-adapter.js",
  "module": "components/index.js",
  "unpkg": "dist/vue-mdc-adapter.js",
  "files": [
    "dist",
    "static",
    "components",
    "LICENSE",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/stasson/vue-mdc-adapter.git"
  },
  "bugs": {
    "url": "https://github.com/stasson/vue-mdc-adapter/issues"
  },
  "scripts": {
    "rollup": "run-s rollup:**",
    "rollup:dev": "cross-env NODE_ENV=development rollup -c",
    "rollup:prod": "cross-env NODE_ENV=production rollup -c",
    "lint": "run-s lint:**",
    "lint:es": "eslint --ext .js,.vue components",
    "lint:style": "stylelint \"components/**/*{.scss,.vue}\" ",
    "clean": "run-s clean:**",
    "clean:dist": "rimraf dist/*",
    "reset:modules": "rimraf node_modules/* && npm install",
    "changelog": "standard-changelog -i CHANGELOG.md -k package.json -w",
    "commitmsg": "validate-commit-msg"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "material-components-web": "0.22.0"
  },
  "peerDependencies": {
    "vue": "^2.4.4"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.5",
    "babel": "^6.23.0",
    "babel-core": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.0",
    "cross-env": "^5.0.5",
    "cz-conventional-changelog": "^2.0.0",
    "eslint": "^4.8.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-html": "^3.2.2",
    "husky": "^0.14.3",
    "npm-run-all": "^4.1.1",
    "postcss": "^6.0.13",
    "postcss-csso": "^2.0.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.50.0",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-filesize": "^1.4.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-sass": "^0.5.3",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-plugin-vue": "^2.5.2",
    "stylelint": "^8.2.0",
    "stylelint-config-recommended-scss": "^2.0.0",
    "stylelint-processor-html": "^1.0.0",
    "stylelint-scss": "^2.1.0",
    "uglify-es": "^3.1.3",
    "validate-commit-msg": "^2.14.0",
    "vue": "^2.4.4",
    "vue-template-compiler": "^2.4.4"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
